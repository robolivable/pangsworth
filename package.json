{
  "name": "pangsworth",
  "version": "3.0.0",
  "description": "Pangsworth at your service anywhere in Madrigal.",
  "author": "https://github.com/robolivable",
  "license": "GPL-3.0",
  "scripts": {
    "build": "webpack",
    "build-dev": "NODE_ENV=development npm run build -- --mode development && npm run generate-licenses",
    "build-release": "NODE_ENV=production npm run build && npm run generate-licenses",
    "jshint": "standard",
    "postinstall": "npm run build-release",
    "test": "jest",
    "generate-licenses": "node ${PWD}/scripts/generate-licenses.js ${PWD}/build ${PWD}/src"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "ag-grid-community": "^27.2.1",
    "ag-grid-react": "^27.2.1",
    "clsx": "^1.1.1",
    "core-js": "^3.21.1",
    "elasticlunr": "0.9.5",
    "fetch-retry": "4.1.1",
    "file-loader": "^6.2.0",
    "idb": "6.1.3",
    "leaflet": "^1.8.0",
    "lunr-languages": "1.9.0",
    "mathjs": "^10.5.1",
    "node-fetch": "^2.6.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@svgr/webpack": "^5.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "fake-indexeddb": "^3.1.3",
    "jest": "^27.0.6",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "standard": "^16.0.4",
    "style-loader": "^3.2.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.8.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "text",
      "lcov",
      "clover"
    ],
    "verbose": true
  },
  "standard": {
    "ignore": [
      "build",
      "test.js"
    ],
    "parser": "babel-eslint",
    "globals": [
      "caches",
      "chrome",
      "idb",
      "fetch",
      "self"
    ]
  }
}
